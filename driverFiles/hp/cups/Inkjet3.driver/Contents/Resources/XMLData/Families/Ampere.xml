							true: HPManualBorderlessStatus in {allowed} and not (HPSystemDuplexing in {true} and HPDuplex in {true} and not HPPaperType in {hp-brochure-paper-180g-glossy,hp-trifold-brochure-paper-glossy,other-brochure-papers,hp-trifold-brochure-paper-matte,hp-professional-brochure-paper-matte}) or HPManualBorderlessStatus in {already};
					</valid-values></setting><setting name="HPManualBorderlessEnabledState" link="Data/JobSettings.xml#/js-container/settings/setting[@name='HPManualBorderlessEnabledState']"><single-valid-value>
								notapplicable-hidden if not Printer has attribute "product/features/feature[@name='supports-manual-borderless']";
								mutable-disabled if HPSelectedPaperSizeIsBorderless in {true} or
													HPPhysicalPaperSizeHasBorderlessAnalog in {false} or
													HPInvalidPaperSize in {true} or
													HPPhysicalPaperSizeIsBanner in {true} or
													(HPSelectedPaperSizeIsBorderless in {false} and HPPhysicalPaperSizeIsManualBorderless in {false}) or
													(HPSystemDuplexing in {true} and HPDuplex in {true} and not HPPaperType in {hp-brochure-paper-180g-glossy,hp-trifold-brochure-paper-glossy,other-brochure-papers,hp-trifold-brochure-paper-matte,hp-professional-brochure-paper-matte});
