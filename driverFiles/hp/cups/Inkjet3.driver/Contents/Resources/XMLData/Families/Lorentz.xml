"no-access" setting-type="private"><single-valid-value>
						true if not HPColorMode in {colorsync,application-managed};
						false otherwise;
					</single-valid-value></setting><setting name="HPPaperType" link="Data/JobSettings.xml#/js-container/settings/setting[@name='HPPaperType']"><valid-values>
						*: /* Check if the paper type exists in paper size's allowed/disallowed lists */
							(
								(not HPPhysicalPaperSize has attribute "allowed-media-types" and not HPPhysicalPaperSize has attribute "disallowed-media-types") or
								(HPPhysicalPaperSize has attribute "allowed-media-types/reference[@name='%*%']") or
								(not HPPhysicalPaperSize has attribute "allowed-media-types" and HPPhysicalPaperSize has attribute "disallowed-media-types" and HPPhysicalPaperSize not has attribute "$Root/capabilities/constrained-media-types/disallowed-media-types[@name=$Self/disallowed-media-types/@ref-name]/reference[@name='%*%']")
							) and
							/* Check if the paper type exists in paper source's allowed/disallowed lists */
							(
								(not HPPaperSource has attribute "allowed-media-types" and not HPPaperSource has attribute "disallowed-media-types") or
								(HPPaperSource has attribute "allowed-media-types/reference[@name='%*%']") or
								(not HPPaperSource has attribute "allowed-media-types" and HPPaperSource has attribute "disallowed-media-types" and HPPaperSource not has attribute "$Root/capabilities/constrained-media-types/disallowed-media-types[@name=$Self/disallowed-media-types/@ref-name]/reference[@name='%*%']")
							);
					</valid-values><default-value>
						hp-premium-plus-photo-paper if HPColorMatchingIsACM in {true} or
								(HPPhysicalPaperSize has attribute "default-media-type[.='hp-premium-plus-photo-paper']") or
								((not HPPhysicalPaperSize has attribute "default-media-type") and 
								(Printer has attribute "capabilities/media-type-pick/@default[.='hp-premium-plus-photo-paper']"));
						inkjet-hagaki if HPPaperSource in {Hagaki} or 
								(HPPhysicalPaperSize has attribute "default-media-type[.='inkjet-hagaki']") or
								((not HPPhysicalPaperSize has attribute "default-media-type") and 
								(Printer has attribute "capabilities/media-type-pick/@default[.='inkjet-hagaki']"));
						* if (HPPhysicalPaperSize has attribute "default-media-type[.='%*%']") or
								((not HPPhysicalPaperSize has attribute "default-media-type") and 
								(Printer has attribute "capabilities/media-type-pick/@default[.='%*%']"));
						plain-paper otherwise;
					</default-value><heuristics><heuristic name="d16e454" heuristic-type="CurrentValueChange" class="CurrentValueChangeSetDefaultHeuristic" source="HPPhysicalPaperSize"/><heuristic name="d16e459" heuristic-type="CurrentValueChange" class="CurrentValueChangeColorSyncAMCAutomaticHeuristic" source="HPColorMode"/><heuristic name="d16e464" heuristic-type="CurrentValueChange" class="CurrentValueChangeACMHeuristic" source="HPColorMatchingIsACM"/></heuristics></setting><setting name="HPPrintQuality" link="Families/Corona.xml#/fm-container/family[@name='corona']/job/settings/setting[@name='HPPrintQuality']"><valid-values>
						best: (HPPaperType has attribute "print-quality-pick/reference[@name='best']") and not
							(HPColorMode in {application-managed} and 
							HPPaperType in {hp-brochure-paper-180g-glossy});
						highest: (HPPaperType has attribute "print-quality-pick/reference[@name='highest']");
						normal: (HPPaperType has attribute "print-quality-pick/reference[@name='%*%']") and
							(not HPColorMode in {application-managed} 
							or HPPaperType in {hp-brochure-paper-180g-glossy});
						*: (HPPaperType has attribute "print-quality-pick/reference[@name='%*%']") and
							(not HPColorMode in {application-managed});
					</valid-values></setting><setting name="HPPaperTypeSupportsBlackInk" link="Families/Corona.xml#/fm-container/family[@name='corona']/job/settings/setting[@name='HPPaperTypeSupportsBlackInk']"><single-valid-value>
						true if (HPPaperType has attribute "media-type-info/@pcl-media-type[.='2']" and HPPrintQuality in {best}) or
							(HPPaperType has attribute "media-type-info/@pcl-media-type[.='4']" and HPPrintQuality in {draft}) or
							(HPPaperType has attribute "media-type-info/@black-only[.='true']");
						false otherwise;
					</single-valid-value></setting></settings><rendering link="Families/Corona.xml#/fm-container/family[@name='corona']/job/rendering"/></job><product link="Families/Corona.xml#/fm-container/family[@name='corona']/product"><features link="Families/Corona.xml#/fm-container/family[@name='corona']/product/features"><feature name="supports-drytime" used="false"/><feature name="persistent-drytime" used="false"/><feature name="supports-ink-density" used="false"/></features></product></family></fm-container>
